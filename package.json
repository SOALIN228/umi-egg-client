{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@rematch/core": "^2.0.0-next.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.24",
    "dayjs": "^1.9.6",
    "es-cookie": "^1.3.2",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "rc-form": "^2.4.11",
    "react": "^16.12.0",
    "react-awesome-swiper": "^1.4.16",
    "react-dom": "^16.12.0",
    "react-icons": "^3.11.0",
    "umi": "^3.2.24",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {}
}
